{
    "builds": [
        {
            "src": "todo/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "todo/wsgi.py"
        }
    ],
    "commands": {
        "build": "py manage.py collectstatic --noinput",
        "postinstall": "py manage.py migrate"
    }
}
